{
  "compilerOptions": {
    "outDir": "./dist",
    "esModuleInterop": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": true,
    "target": "es5",
    "lib": ["es2015", "es6"],
    "module": "commonjs",
    "typeRoots": [
      "node_modules/@types", 
      "node_modules/@atomicmp"
    ],
    "noImplicitAny": true
  },
  "include": ["./src/**/*"]
}
